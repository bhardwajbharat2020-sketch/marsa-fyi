{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server/server.js"
    },
    {
      "src": "/static/js/(.*)",
      "dest": "/client/build/static/js/$1"
    },
    {
      "src": "/static/css/(.*)",
      "dest": "/client/build/static/css/$1"
    },
    {
      "src": "/static/media/(.*)",
      "dest": "/client/build/static/media/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/client/build/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/server.js"
    }
  ]
}
